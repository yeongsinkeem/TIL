import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String a = sc.next();
    
        for( char c : a.toCharArray() ) {
            // 소문자라면
            if( c >= 97 ) {
                System.out.print( (char)(c - 97 + 65) );
            }
            // 대문자라면
            else {
                System.out.print( (char)(c + 32) );
            }
        }
    }
}
