import java.util.*;

class Solution {
    public String[] solution(String my_string) {
        String[] answer = new String[ my_string.length() ];
        
        // 1. 접미사 구분
        for(int i = 0; i < my_string.length(); i++) {
            answer[i] = my_string.substring( i );
        }
        // 2. 정렬
        Arrays.sort(answer);
        
        return answer;
    }
}
