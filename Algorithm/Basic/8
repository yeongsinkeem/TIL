import java.util.*;

class Solution {
    public String[] solution(String[] names) {
        List<String> arr = new ArrayList<>();
        
        for(int i = 0; i < names.length; i+= 5) {
            arr.add(names[i]);
        }
        
        String[] answer = new String[arr.size()];
        
        for(int i = 0; i < answer.length; i++){
            answer[i] = arr.get(i);
        }
        return answer;
    }
}
